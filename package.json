{
  "name": "owasp-top-ten-workshop",
  "private": true,
  "type": "module",
  "workspaces": [
    "src/*"
  ],
  "version": "1.0.0",
  "license": "CC-BY-SA-4.0",
  "author": "Liana Pigeot <liana.pigeot@nearform.com>",
  "description": "OWASP Top 10 Security Vulnerabilities Workshop",
  "main": "index.js",
  "imports": {
    "#/*": "src/*"
  },
  "scripts": {
    "build": "slidev build",
    "start": "slidev --open",
    "export": "slidev export",
    "db:up": "docker-compose up -d",
