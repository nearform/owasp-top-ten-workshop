    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-sql": "^3.2.1",
    "husky": "^9.1.6",
    "lint-staged": "^15.4.3",
    "nodemon": "^3.1.4",
    "postgrator-cli": "^8.1.0",
    "prettier": "^3.3.3"
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint --cache --fix"
