    "@nearform/sql": "^1.10.5",
    "@sinclair/typebox": "^0.33.11",
    "@slidev/cli": "^0.49.29",
    "@vueuse/shared": "^11.0.3",
    "bcrypt": "^5.1.1",
    "desm": "^1.3.1",
    "env-schema": "^6.0.0",
    "fastify": "^5.2.1",
    "fastify-plugin": "^5.0.1",
    "fastify-print-routes": "^4.0.1",
    "http-errors": "^2.0.0",
    "md5": "^2.3.0",
    "pg": "^8.13.0",
    "pino-pretty": "^11.2.2",
    "slidev-theme-nearform": "^2.0.0",
    "undici": "^7.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.1",
